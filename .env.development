# 开发环境配置
ENV=development

# 服务端口
APP_PORT=5512
MCP_PORT=5513

# 服务器配置
SERVER_BASE_URL=http://localhost:5512
LOG_LEVEL=debug

# Redis配置 (本地)
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_DB=0

# MinIO配置 (本地)
MINIO_ENDPOINT=localhost:9000
MINIO_ACCESS_KEY=minioadmin
MINIO_SECRET_KEY=minioadmin123
MINIO_SECURE=false
MINIO_BUCKET_NAME=images

# 网络配置
NETWORK_NAME=default

# 资源配置 (开发环境较宽松)
MEMORY_LIMIT=1G
CPU_LIMIT=1.0
MEMORY_RESERVE=256M
CPU_RESERVE=0.2
WORKERS=2
WORKER_CONCURRENCY=2

# 健康检查
HEALTH_START_PERIOD=30

# Google凭证路径
GOOGLE_CREDS_PATH=./google_oauth